DATA: File is regresija.dat;

VARIABLE:

NAMES ARE ID EKST_C UGOD_C SAV_C NE_C OTV_C HUM
IMP NEG_VAL COMPLEX TIP POS_VAL ADVER
SAV EKST A2 N2 OTV SEU;

USEVARIABLES ARE NE_C  
EKST N2 SEU;

MISSING ARE ALL (-99);

CLUSTER IS id;

BETWEEN ARE NE_C;


ANALYSIS:
TYPE = TWOLEVEL;
ESTIMATOR = BAYES;
PROCESSORS = 12;  
ALGORITHM=GIBBS(RW); 
FBITERATIONS = 200000;


MODEL: 

%WITHIN%
EKST; 
N2; 
SEU; 

EKST WITH N2; 
EKST WITH SEU; 
N2 WITH SEU;


%BETWEEN%  
NE_C;

SEU ON EKST (b1); 
SEU ON N2 (b2); 

  
EKST ON NE_C (a12); 
N2 ON NE_C (a13);

SEU ON NE_C; 


EKST WITH N2; 


MODEL CONSTRAINT:

NEW (ind_NE ind_NS);


ind_NE = a12*b1;
ind_NS = a13*b2;



OUTPUT: STANDARDIZED TECH8; 